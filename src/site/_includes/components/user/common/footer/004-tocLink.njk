<script>
var heading = document.querySelectorAll(".cm-s-obsidian>*[id]");
for(var i = 0; i < heading.length; i++){
    var a = document.createElement("a");
    a.href = "#" + heading[i].id;
    a.innerText = heading[i].innerText;
    heading[i].innerText = "";
    heading[i].appendChild(a);
}
</script>