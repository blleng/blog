<div id="forest-graph">
    <div class="forest-body">
        {% for row in userComputed.forest.trees %}
        <div class="forest-row">
            {% for tree in row %}
            {% if tree[0] == 0 %}
            <span class="tree plane">
                <span></span>
            </span>
            {% else %}
            <a class="tree" href="{{tree[1]}}" title="{{tree[2]}}">
                {% if tree[0] == 1 %}<img src="data:image/svg+xml;base64," alt="" style="width: auto;height: {{5 + (10 * tree[3])}}px;" />
                {% elif tree[0] == 2 %}<img src="data:image/svg+xml;base64," alt="" style="width: auto;height: {{5 + (10 * tree[3])}}px;" />
                {% elif tree[0] == 3 %}<img src="data:image/svg+xml;base64," alt="" style="width: auto;height: {{5 + (10 * tree[3])}}px;" />
                {% elif tree[0] == 4 %}<img src="data:image/svg+xml;base64," alt="" style="width: auto;height: {{5 + (10 * tree[3])}}px;" />
                {% elif tree[0] == 5 %}<img src="data:image/svg+xml;base64," alt="" style="width: auto;height: {{5 + (10 * tree[3])}}px;" />
                {% elif tree[0] == 6 %}<img src="data:image/svg+xml;base64," alt="" style="width: auto;height: {{5 + (10 * tree[3])}}px;" />
                {% endif %}
            </a>
            {% endif %}
            {% endfor %}
        </div>
        {% endfor %}
    </div>
    <div class="forest-legends">
        {% for count in userComputed.forest.legends %}
        <div class="stat" icon="{{count.icon}}" > {{count.count}} {{count.label}}</div>
        {% endfor %}
    </div>
</div>